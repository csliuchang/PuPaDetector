{
    "model": {
        "type": "FCOSOBB",
        "backbone": {
            "type": "ResNet",
            "depth": 18,
            "out_levels": [1, 2, 3, 4, 5],
            "num_classes": null
        },
        "neck" : {
            "type": "FPN",
            "in_channels": [64, 64, 128, 256, 512],
            "out_channels": 64,
            "start_level": 1,
            "add_extra_convs": true,
            "extra_convs_on_inputs": false,
            "num_outs": 5,
            "relu_before_extra_convs": true
        },
        "bbox_head": {
            "type": "OBBFCOSHead",
            "num_classes": 1,
            "in_channels": 64,
            "stacked_convs": 4,
            "feat_channels": 64,
            "strides": [8, 16, 32, 64, 128],
            "scale_theta": true,
            "loss_cls": {
                "type":"FocalLoss",
                "use_sigmoid": true,
                "gamma": 2.0,
                "alpha": 0.25,
                "loss_weight": 1.0
            },
            "loss_bbox": {
                "type": "GWDLoss",
                "loss_weight": 1.0
            },
            "loss_centerness": {
                "type": "CrossEntropyLoss",
                "use_sigmoid": true,
                "loss_weight": 1.0
            }
        }
    },
    "train_cfg": {
        "assigner": {"type":"MaxIoUAssigner",
                "pos_iou_thr": 0.5,
                "neg_iou_thr": 0.4,
                "min_pos_iou": 0,
                "ignore_iof_thr": -1},
                "iou_calculator": {"type":"RBboxOverlaps2D"},
        "sampler_cfg": {"type":"PseudoSampler",
                        "sampling": false
        },
        "allowed_border": -1,
        "pos_weight": -1,
        "debug": false,
        "stage_loss_weights": [1.0, 1.0],
        "num_anchors": 27,
        "auto_anchors": false

    },
    "test_cfg": {
        "nms_pre": 100,
        "score_thr": 0.5,
        "max_per_img": 100,
        "nms": {"type": "rnms",
                     "iou_thr": 0.3
                    },
        "merge_cfg": {
             "nms_pre": 100,
             "score_thr": 0.5,
             "nms": {"type": "rnms",
                     "iou_thr": 0.3
                    },
            "max_per_img": 100
            }
    }
}

